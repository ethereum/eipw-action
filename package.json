{
  "name": "eipw-action",
  "version": "0.1.0",
  "description": "A GitHub action to run eipw and display the results.",
  "repository": "github:ethereum/eipw-action",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "esbuild src/action.ts --bundle --platform=node --target=node16 --outfile=dist/index.js && echo { \"type\": \"commonjs\" } > dist/package.json"
    "test": "prettier --check src/**/*.ts && exit 1",
    "format": "prettier --write src/**/*.ts"
  },
  "author": "",
  "license": "MPL-2.0",
  "dependencies": {
    "@actions/core": "^1.9.0",
    "@actions/github": "^5.0.3",
    "@octokit/plugin-throttling": "^4.1.0",
    "@octokit/webhooks-types": "^6.2.2",
    "eipw-lint-js": "^0.1.5"
  },
  "devDependencies": {
    "esbuild": "0.15.5",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  }
}
